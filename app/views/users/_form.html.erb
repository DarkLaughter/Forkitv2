<%= form_for @user do |f| %>

<%= f.label :name %><br/>
<%= f.text_field :name %><br/>

<%= f.label :email %><br/>
<%= f.text_field :email %><br/>

<%= f.label :username %><br/>
<%= f.text_field :username %><br/>

<%= f.label :password %><br/>
<%= f.text_field :password %><br/>

<%= f.label :password_confirmation %><br/>
<%= f.text_field :password_confirmation %><br/>

<%= f.label :img_url %><br/>
<%= f.text_field :img_url %><br/>

<%= f.submit %>
    
<% end %>