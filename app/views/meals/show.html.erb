<div class="mp-layout">

    <div class="meal-display">
        <h1><%= @meal.name %></h1>
        <img class='meal-profile' src="<%= @meal.img_url %>" alt="<%= @meal.name%>"/>
        <h2><%= "Craving #{@meal.cuisine}?" %></h2>
        

      


    </div>
    <div class="ingredients-display">
        <h2> Ingredients </h2>
        <% @meal.list_ingredients.each do |i| %>
            <li><%= i %></li>
        <% end %>
    </div>



   
</div>



