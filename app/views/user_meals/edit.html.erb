<%= form_for @user_meal do |f| %>

<%= f.label :date %><br/>
<%= f.date_field :date %><br/>

<%= f.label :time %>
<%= f.time_field :time %>

<%= f.label :meal_type %><br/>
<%= f.text_field :meal_type %><br/>

<%= f.label :user_id %><br/>
<%= f.collection_select  :user_id, User.all, :id, :name %><br/>

<%= f.label :meal_id %><br/>
<%= f.collection_select  :meal_id, Meal.all, :id, :name %><br/>

<%= f.submit %>    
<% end %>